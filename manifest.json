{

  "manifest_version": 2,
  "name": "OpenNear",
  "version": "1.0",

  "description": "Open a tab near the current tab",
  "homepage_url": "https://github.com/beyondlov1/open-near",
  
  "permissions": [
    "activeTab",
    "sessions",
    "tabs"
  ],
  "background": {
    "page": "/background/background.html"
  }
 
}

