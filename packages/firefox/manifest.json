{

  "manifest_version": 3,
  "name": "OpenNear",
  "version": "1.2.1",

  "description": "Open a tab near the current tab",
  "homepage_url": "https://github.com/beyondlov1/open-near",

  "browser_specific_settings": {
    "gecko": {
      "id": "opennear@beyond.com",
      "strict_min_version": "50.0"
    }
  },

  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["/background/background.js"]
  }
 
}

