{

  "manifest_version": 3,
  "name": "OpenNear",
  "version": "1.2.1",

  "description": "Open a tab near the current tab",
  "homepage_url": "https://github.com/beyondlov1/open-near",


  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "/background/background.js"
  }
}

